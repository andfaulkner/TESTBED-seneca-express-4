<div class="starter-template">
	<div id="display_content_area">
		<div class="container">
		  <h2>Striped Rows</h2>
		  <p>The .table-striped class adds zebra-stripes to a table:</p>

		  <table class="table table-striped">
		    <thead>
		      <tr>
		        <th>Firstname</th>
		        <th>Lastname</th>
		        <th>Roles</th>
		      </tr>
		    </thead>
		    <tbody id="display_page_table_data">

			    {collHtml|s}

		    </tbody>
		  </table>

		</div>
  </div>
</div>
